<tr>
    <td>
        <%= friend.first_name %>
    </td>
    <td>
        <%= friend.last_name %>
    </td>
    <td>
        <%= friend.email %>
    </td>
    <td>
        <%= friend.phone %>
    </td>
    <td>
        <%= friend.twitter %>
    </td>
    <td>
        <%= link_to "View", friend %>
        <%= link_to 'Edit', [:edit, friend] %>
        <%= link_to 'Destroy', friend, :method => :delete, :confirm => "Are you sure?" %>
    </td>
</tr>
